{% extends 'base.html.twig' %}

{% block title %}AnalysisType index{% endblock %}

{% block body %}
    <h1>AnalysisType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Analysis_name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for analysis_type in analysis_types %}
            <tr>
                <td>{{ analysis_type.id }}</td>
                <td>{{ analysis_type.analysisName }}</td>
                <td>
                    <a href="{{ path('analysis_type_show', {'id': analysis_type.id}) }}">show</a>
                    <a href="{{ path('analysis_type_edit', {'id': analysis_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('analysis_type_new') }}">Create new</a>
{% endblock %}
